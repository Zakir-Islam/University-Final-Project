@model CreateRoleModel
@{
    ViewData["Title"] = "Delete";
}



<h3 style="color:red">Are you sure you want to delete this?</h3>
<div>
    <h4>ExamResult</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
              Role Id
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Role_ID)
        </dd>
        <dt class="col-sm-2">
            Name
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Role_Name)
        </dd>
      
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="Role_ID" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index" class="btn btn-primary">Back to List</a>
    </form>
</div>

