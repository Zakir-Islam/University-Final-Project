@model IEnumerable<University_Final_Project.Models.Subject>

@{
    ViewData["Title"] = "StudentAttendenceDash";
}



<table class="table">
    <thead>
        <tr>
            <th>
                Subject
            </th>

            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.corse_title)
                </td>

                <td>
                    <a asp-action="index" asp-route-subjectID="@item.Subject_Id" class="btn" style="background-color:aquamarine;color:white">View Attendeance Details</a>

                </td>
            </tr>
        }
    </tbody>
</table>
