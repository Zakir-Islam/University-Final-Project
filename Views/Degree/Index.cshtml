@model IEnumerable<University_Final_Project.Models.Degree>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create" class="btn btn-primary">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.departmentFid)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DegreeName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.departmentFid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DegreeName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.DegreeId " class="btn btn-primary">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.DegreeId" class="btn btn-primary">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.DegreeId" class="btn btn-danger">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
