@model DashboardCounter
<div class="row mt">
    <div class="col-lg-12">
        <!-- CHART PANELS -->
        <!--  SECOND ROW OF PANELS -->
        <div class="row">

            <!--  /col-md-4 -->
            <div class="col-md-4 col-sm-4 mb">
                <div class="darkblue-panel pn">
                    <div class="darkblue-header">
                        <h5>Teachers</h5>
                    </div>
                    <h1 class="mt"><i class="fa fa-user fa-3x"></i></h1>

                    <footer>
                        <div class="centered">
                            <h5>( @Model.NoTch )</h5>
                        </div>
                    </footer>
                </div>
                <!--  /darkblue panel -->
            </div>
            <!-- /col-md-4 -->
            <!--  /col-md-4 -->
            <div class="col-md-4 col-sm-4 mb">
                <div class="darkblue-panel pn " style="background:red;">
                    <div class="darkblue-header">
                        <h5>Admins</h5>
                    </div>
                    <h1 class="mt"><i class="fa fa-user fa-3x"></i></h1>
                    <footer>
                        <div class="centered">
                            <h5>( @Model.NoAdm )</h5>
                        </div>
                    </footer>

                </div>
                <!--  /darkblue panel -->
            </div>
            <!-- /col-md-4 -->
            <!--  /col-md-4 -->
            <div class="col-md-4 col-sm-4 mb">
                <div class="darkblue-panel pn" style="background:green;">
                    <div class="darkblue-header">
                        <h5> Students</h5>
                    </div>
                    <h1 class="mt"><i class="fa fa-user fa-3x"></i></h1>
                    <footer>
                        <div class="centered">
                            <h5>( @Model.NoStd )</h5>
                        </div>
                    </footer>

                </div>
                <!--  /darkblue panel -->
            </div>
            <!-- /col-md-4 -->
        </div>
        <!-- /row - SECOND ROW OF PANELS -->
        <!--  SECOND ROW OF PANELS -->
        <div class="row">

            <!--  /col-md-4 -->
            <div class="col-md-4 col-sm-4 mb">
                <div class="darkblue-panel pn" style="background: rgb(78, 205, 196);">
                    <div class="darkblue-header">
                        <h5>Degree</h5>
                    </div>
                    <h1 class="mt"><i class="glyphicon glyphicon-certificate fa-3x"></i></h1>

                    <footer>
                        <div class="centered">
                            <h5>( @Model.NoDegr )</h5>
                        </div>
                    </footer>
                </div>
                <!--  /darkblue panel -->
            </div>
            <!-- /col-md-4 -->
            <!--  /col-md-4 -->
            <div class="col-md-4 col-sm-4 mb">
                <div class="darkblue-panel pn " style="background:yellow;">
                    <div class="darkblue-header">
                        <h5>Departments</h5>
                    </div>
                    <h1 class="mt"><i class="glyphicon glyphicon-star fa-3x"></i></h1>
                    <footer>
                        <div class="centered">
                            <h5>( @Model.NoDep)</h5>
                        </div>
                    </footer>

                </div>
                <!--  /darkblue panel -->
            </div>
            <!-- /col-md-4 -->
            <!--  /col-md-4 -->
            <div class="col-md-4 col-sm-4 mb">
                <div class="darkblue-panel pn" style="background:blue;">
                    <div class="darkblue-header">
                        <h5>Subjects</h5>
                    </div>
                    <h1 class="mt"><i class="glyphicon glyphicon-book fa-3x"></i></h1>
                    <footer>
                        <div class="centered">
                            <h5>( @Model.NoSub )</h5>
                        </div>
                    </footer>

                </div>
                <!--  /darkblue panel -->
            </div>
            <!-- /col-md-4 -->
        </div>
        <!-- /row - SECOND ROW OF PANELS -->

    </div>
</div>
