@model IEnumerable<University_Final_Project.Models.Subject>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Subject_Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.corse_title)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.credit_hours)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Subject_Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.corse_title)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.credit_hours)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Subject_Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Subject_Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Subject_Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
